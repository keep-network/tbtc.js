{
  "compilerOptions": {
    "module": "es6",
    "target": "es6",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "maxNodeModuleJsDepth": 2,
    "esModuleInterop": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "checkJs": true,
    "lib": [
      "es2019"
    ]
  }
}
